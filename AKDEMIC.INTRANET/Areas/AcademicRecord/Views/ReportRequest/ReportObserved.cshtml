@{
    ViewBag.Title = "Reporte de Tràmites Observados";
    ViewData["Breadcrumbs"] = new[] {
    new BreadcrumbViewData { Name = "Reporte de Estudiantes",  Action = "Student" , Controller = "IntranetReport", Area = "Report" },
new BreadcrumbViewData { Name = (string)ViewBag.Title }
};
}
<div class="m-content">
    <div class="m-portlet">
        <div class="m-portlet__head">
            <div class="m-portlet__head-caption">
                <div class="m-portlet__head-title">
                    <a asp-controller="IntranetReport" asp-area="Report" asp-action="Student" class="btn btn-primary m-btn m-btn--icon m-btn--icon-only"><i class="la la-arrow-left"></i></a>
                    &nbsp;
                    <h3 class="m-portlet__head-text">
                        Reporte de Trámites Observados
                    </h3>
                </div>
            </div>
        </div>
        <div class="m-portlet__body">
            <div class="m-form m-form--label-align-right m--margin-top-20 m--margin-bottom-30">
                <div class="row align-items-center">
                    <div class="col-xl-12">
                        <div class="row">
                            <div class="form-group m-form__group col-md-3">
                                <label>Tipo</label>
                                <select class="form-control m-select2" id="type" style="padding-top:0px !important">
                                    <option value="">Todos</option>
                                    @foreach(var item in ConstantHelpers.RECORDS.VALUES)
                                    {
                                        <option value="@item.Key">@item.Value</option>
                                    }
                                </select>
                            </div>
                            <div class="form-group m-form__group col-md-3" style="padding-top:0px !important">
                                <label>Carreras</label>
                                <select class="form-control m-select2" id="careerId">
                                    <option value="">Todos</option>
                                </select>
                            </div>
                            <div class="form-group m-form__group col-md-3" style="padding-top:0px !important">
                                <label>Fec. Inicio</label>
                                <input class="form-control m-input" placeholder="Seleccione fecha de inicio" id="start" />
                            </div>
                            <div class="form-group m-form__group col-md-3" style="padding-top:0px !important;">
                                <label>Fec. Fin</label>
                                <input class="form-control m-input" placeholder="Seleccione fecha fin" id="end" />
                            </div>
                        </div>
                    </div>
                </div>
                <table id="datatable" class="table table-striped"></table>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/js/areas/academicrecord/reportrequest/reportobserved.js" asp-append-version="true"></script>
}