@{
    ViewBag.Title = "Examenes Subsanacion";
    ViewData["Breadcrumbs"] = new[] {
        new BreadcrumbViewData { Name = ViewBag.Title.ToString(), Area = "Teacher", Controller = "CorrectionExam", Action = "Index" }
    };
}

<div class="m-content">
    <div class="m-portlet">
        <div class="m-portlet__head">
            <div class="m-portlet__head-caption">
                <div class="m-portlet__head-title">
                    <h3 class="m-portlet__head-text">
                        @ViewBag.Title
                    </h3>
                </div>
            </div>
        </div>
        <div class="m-portlet__body">
            <div class="m-form--label-align-right m--margin-top-20 m--margin-bottom-30">
                <div class="row align-items-center">
                    <div class="col-xl-8 order-2 order-xl-1">
                        <div class="row">
                            <div class="form-group m-form__group col-xl-3">
                                <label>Periodo</label>
                                <select class="select2-terms m-select2"></select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="table-container">
                <table class="table table-hover table-striped table-bordered" id="exams-datatable"></table>
            </div>
        </div>
    </div>
</div>


@section Scripts {
        <script src="~/js/areas/teacher/correctionexam/index.js" asp-append-version="true"></script>
}