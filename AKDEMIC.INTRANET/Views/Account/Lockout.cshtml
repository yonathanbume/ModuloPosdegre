@model string
@{
    ViewData["Title"] = "Bloqueado";
}
<div class="swal2-popup swal2-modal" style="display: flex;width:100% !important;height:100%">
    <div class="swal2-header">
        <div class="swal2-icon swal2-error swal2-icon-show" style="display: flex;">
            <span class="swal2-x-mark">
                <span class="swal2-x-mark-line-left"></span>
                <span class="swal2-x-mark-line-right"></span>
            </span>
        </div>
        <h2 class="swal2-title" id="swal2-title" style="display: flex;">¡Tu usuario ha sido bloqueado!</h2>
    </div>
    <div class="swal2-content">
        <div id="swal2-content" class="swal2-html-container" style="display: block;">@Model</div>
    </div>
    @*<div class="swal2-actions">
        <form asp-area="" asp-controller="Account" asp-action="Logout" method="post" class="navbar-right">
            <button type="submit" class="swal2-confirm swal2-styled btn btn-primary">Cerrar Sesión</button>
        </form>
    </div>*@
</div>