                                <select asp-for="SexEnjoyLevel" class="form-control m-input" required>
                                    @foreach (var type in AKDEMIC.INTRANET.Helpers.ConstantHelpers.EVALUATION.SDS.VALUES)
                                    {
                                        <option value="@(type.Key)">@(type.Value)</option>
                                    }
                                </select>
                            </div>
                        </div>

                        <div class="form-group m-form__group row">
                            <div class="col-md-4 m-form__group-sub">
                                <label class="form-control-label">He notado que he perdido peso</label>
                                <select asp-for="WeightKnowledgeLevel" class="form-control m-input" required>
                                    @foreach (var type in AKDEMIC.INTRANET.Helpers.ConstantHelpers.EVALUATION.SDS.VALUES)
                                    {
                                        <option value="@(type.Key)">@(type.Value)</option>
                                    }
                                </select>
                            </div>
                            <div class="col-md-4 m-form__group-sub">
                                <label class="form-control-label">Tengo problemas de estreñimiento</label>
                                <select asp-for="ConstipationProblemLevel" class="form-control m-input" required>
                                    @foreach (var type in AKDEMIC.INTRANET.Helpers.ConstantHelpers.EVALUATION.SDS.VALUES)
                                    {
                                        <option value="@(type.Key)">@(type.Value)</option>
                                    }
                                </select>
                            </div>
                            <div class="col-md-4 m-form__group-sub">
                                <label class="form-control-label">Mi corazón late más rápido de lo normal</label>
                                <select asp-for="HeartProblemLevel" class="form-control m-input" required>
                                    @foreach (var type in AKDEMIC.INTRANET.Helpers.ConstantHelpers.EVALUATION.SDS.VALUES)
                                    {
                                        <option value="@(type.Key)">@(type.Value)</option>
                                    }
                                </select>
                            </div>
                        </div>

                        <div class="form-group m-form__group row">
                            <div class="col-md-4 m-form__group-sub">
