    `json_data` longtext CHARACTER SET utf8mb4 NULL,
    `id_tipo` int NOT NULL,
    `QuibukAuthorityTypeid` int NULL,
    `created_at` datetime NOT NULL,
    `updated_at` datetime NOT NULL,
    CONSTRAINT `PK_QuibukAuthorities` PRIMARY KEY (`id`),
    CONSTRAINT `FK_QuibukAuthorities_QuibukAuthorityTypes_QuibukAuthorityTypeid` FOREIGN KEY (`QuibukAuthorityTypeid`) REFERENCES `Library_QuibukAuthorityTypes` (`id`)
) CHARACTER SET=utf8mb4;

