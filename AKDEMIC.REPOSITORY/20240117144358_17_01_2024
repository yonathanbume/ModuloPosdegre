CREATE TABLE `Evaluation_CompanyInterests` (
    `Id` char(36) COLLATE ascii_general_ci NOT NULL,
    `CompanyId` char(36) COLLATE ascii_general_ci NOT NULL,
    `ProjectId` char(36) COLLATE ascii_general_ci NOT NULL,
    `Date` datetime NOT NULL,
    CONSTRAINT `PK_EvaluationCompanyInterests` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_EvaluationCompanyInterests_Companies_CompanyId` FOREIGN KEY (`CompanyId`) REFERENCES `JobExchange_Companies` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_EvaluationCompanyInterests_EvaluationProjects_ProjectId` FOREIGN KEY (`ProjectId`) REFERENCES `Evaluation_Projects` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

CREATE TABLE `Evaluation_ProjectActivities` (
    `Id` char(36) COLLATE ascii_general_ci NOT NULL,
    `ProjectId` char(36) COLLATE ascii_general_ci NOT NULL,
    `Budget` decimal(18,2) NOT NULL,
    `Name` longtext CHARACTER SET utf8mb4 NULL,
    `StartDate` datetime NOT NULL,
    `EndDate` datetime NOT NULL,
    CONSTRAINT `PK_EvaluationProjectActivities` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_EvaluationProjectActivities_EvaluationProjects_ProjectId` FOREIGN KEY (`ProjectId`) REFERENCES `Evaluation_Projects` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

