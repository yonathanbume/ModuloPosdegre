    CONSTRAINT `FK_Visits_AspNetUsers_UserToVisitId` FOREIGN KEY (`UserToVisitId`) REFERENCES `AspNetUsers` (`Id`),
    CONSTRAINT `FK_Visits_Dependencies_DependencyId` FOREIGN KEY (`DependencyId`) REFERENCES `DocumentaryProcedure_Dependencies` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Visits_VisitorInformations_VisitorInformationId` FOREIGN KEY (`VisitorInformationId`) REFERENCES `VisitManagement_VisitorInformations` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

