    `StaticType` int NULL,
    `HasPicture` smallint NOT NULL,
    `FacultyId` char(36) COLLATE ascii_general_ci NULL,
    `DeletedAt` datetime NULL,
    `DeletedBy` longtext CHARACTER SET utf8mb4 NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_Procedures` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_Procedures_Classifiers_ClassifierId` FOREIGN KEY (`ClassifierId`) REFERENCES `EconomicManagement_Classifiers` (`Id`),
    CONSTRAINT `FK_Procedures_Concepts_ConceptId` FOREIGN KEY (`ConceptId`) REFERENCES `EconomicManagement_Concepts` (`Id`),
    CONSTRAINT `FK_Procedures_Dependencies_DependencyId` FOREIGN KEY (`DependencyId`) REFERENCES `DocumentaryProcedure_Dependencies` (`Id`),
    CONSTRAINT `FK_Procedures_Dependencies_StartDependencyId` FOREIGN KEY (`StartDependencyId`) REFERENCES `DocumentaryProcedure_Dependencies` (`Id`),
    CONSTRAINT `FK_Procedures_Faculties_FacultyId` FOREIGN KEY (`FacultyId`) REFERENCES `Enrollment_Faculties` (`Id`),
    CONSTRAINT `FK_Procedures_ProcedureCategories_ProcedureCategoryId` FOREIGN KEY (`ProcedureCategoryId`) REFERENCES `DocumentaryProcedure_ProcedureCategories` (`Id`),
    CONSTRAINT `FK_Procedures_ProcedureSubcategories_ProcedureSubcategoryId` FOREIGN KEY (`ProcedureSubcategoryId`) REFERENCES `DocumentaryProcedure_ProcedureSubcategories` (`Id`)
) CHARACTER SET=utf8mb4;

