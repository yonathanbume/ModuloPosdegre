    `RegisterDate` datetime NOT NULL,
    `Status` int NOT NULL,
    `RacialIdentity` tinyint unsigned NOT NULL,
    `HasGraduatedSurveyCompleted` smallint NOT NULL,
    `EnrollmentFeeId` char(36) COLLATE ascii_general_ci NULL,
    `Condition` tinyint unsigned NOT NULL DEFAULT 1,
    `InitialBenefit` tinyint unsigned NOT NULL,
    `Benefit` tinyint unsigned NOT NULL,
    `MoodleId` int NULL,
    `StudentInformationId` char(36) COLLATE ascii_general_ci NULL,
    `StudentScaleId` char(36) COLLATE ascii_general_ci NULL,
    `RelationId` varchar(50) CHARACTER SET utf8mb4 NULL,
    `DeletedAt` datetime NULL,
    `DeletedBy` longtext CHARACTER SET utf8mb4 NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_Students` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_Students_AcademicAgreements_AcademicAgreementId` FOREIGN KEY (`AcademicAgreementId`) REFERENCES `AcademicExchange_AcademicAgreements` (`Id`),
    CONSTRAINT `FK_Students_AcademicPrograms_AcademicProgramId` FOREIGN KEY (`AcademicProgramId`) REFERENCES `Generals_AcademicPrograms` (`Id`),
    CONSTRAINT `FK_Students_AdmissionTypes_AdmissionTypeId` FOREIGN KEY (`AdmissionTypeId`) REFERENCES `Admission_AdmissionTypes` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Students_AspNetUsers_UserId` FOREIGN KEY (`UserId`) REFERENCES `AspNetUsers` (`Id`),
    CONSTRAINT `FK_Students_Campuses_CampusId` FOREIGN KEY (`CampusId`) REFERENCES `Enrollment_Campuses` (`Id`),
    CONSTRAINT `FK_Students_Careers_CareerId` FOREIGN KEY (`CareerId`) REFERENCES `Generals_Careers` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Students_Curriculums_CurriculumId` FOREIGN KEY (`CurriculumId`) REFERENCES `Enrollment_Curriculums` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Students_EnrollmentFees_EnrollmentFeeId` FOREIGN KEY (`EnrollmentFeeId`) REFERENCES `Enrollment_EnrollmentFees` (`Id`),
    CONSTRAINT `FK_Students_MedicalRecords_MedicalRecordId` FOREIGN KEY (`MedicalRecordId`) REFERENCES `InstitutionalWelfare_MedicalRecords` (`Id`),
    CONSTRAINT `FK_Students_PsychologicalRecords_PsychologicalRecordId` FOREIGN KEY (`PsychologicalRecordId`) REFERENCES `Intranet_PsychologicalRecords` (`Id`),
    CONSTRAINT `FK_Students_Scholarships_ScholarshipId` FOREIGN KEY (`ScholarshipId`) REFERENCES `AcademicExchange_Scholarships` (`Id`),
    CONSTRAINT `FK_Students_Specialties_SpecialtyId` FOREIGN KEY (`SpecialtyId`) REFERENCES `Generals_Specialties` (`Id`),
    CONSTRAINT `FK_Students_StudentScales_StudentScaleId` FOREIGN KEY (`StudentScaleId`) REFERENCES `Generals_StudentScales` (`Id`),
    CONSTRAINT `FK_Students_Terms_AdmissionTermId` FOREIGN KEY (`AdmissionTermId`) REFERENCES `Enrollment_Terms` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Students_Terms_FirstEnrollmentTermId` FOREIGN KEY (`FirstEnrollmentTermId`) REFERENCES `Enrollment_Terms` (`Id`),
    CONSTRAINT `FK_Students_Terms_GraduationTermId` FOREIGN KEY (`GraduationTermId`) REFERENCES `Enrollment_Terms` (`Id`)
) CHARACTER SET=utf8mb4;

