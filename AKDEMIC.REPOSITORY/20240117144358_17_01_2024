    `Id` char(36) COLLATE ascii_general_ci NOT NULL,
    `StartDateTime` datetime NOT NULL,
    `EndDateTime` datetime NOT NULL,
    `Type` tinyint unsigned NOT NULL DEFAULT 1,
    `Duration` decimal(18,2) NOT NULL,
    `Title` longtext CHARACTER SET utf8mb4 NULL,
    `InterestGroupId` char(36) COLLATE ascii_general_ci NOT NULL,
    `HangoutCreatorEmail` longtext CHARACTER SET utf8mb4 NULL,
    `HangoutLink` longtext CHARACTER SET utf8mb4 NULL,
    `GoogleEventId` longtext CHARACTER SET utf8mb4 NULL,
    `UserId` varchar(255) CHARACTER SET utf8mb4 NULL,
    `DeletedAt` datetime NULL,
    `DeletedBy` longtext CHARACTER SET utf8mb4 NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_Conferences` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_Conferences_AspNetUsers_UserId` FOREIGN KEY (`UserId`) REFERENCES `AspNetUsers` (`Id`),
    CONSTRAINT `FK_Conferences_InterestGroups_InterestGroupId` FOREIGN KEY (`InterestGroupId`) REFERENCES `InterestGroup_InterestGroups` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

