CREATE TABLE `EconomicManagement_Payments` (
    `Id` char(36) COLLATE ascii_general_ci NOT NULL,
    `ConceptId` char(36) COLLATE ascii_general_ci NULL,
    `CurrentAccountId` char(36) COLLATE ascii_general_ci NULL,
    `EntityId` char(36) COLLATE ascii_general_ci NULL,
    `InvoiceId` char(36) COLLATE ascii_general_ci NULL,
    `ParentPaymentId` char(36) COLLATE ascii_general_ci NULL,
    `TermId` char(36) COLLATE ascii_general_ci NULL,
    `ExternalUserId` char(36) COLLATE ascii_general_ci NULL,
    `UserId` varchar(255) CHARACTER SET utf8mb4 NULL,
    `BankIdentifier` int NOT NULL AUTO_INCREMENT,
    `BankAgentCode` longtext CHARACTER SET utf8mb4 NULL,
    `BankCashierCode` longtext CHARACTER SET utf8mb4 NULL,
    `BankCondition` longtext CHARACTER SET utf8mb4 NULL,
    `Description` longtext CHARACTER SET utf8mb4 NULL,
    `Discount` decimal(18,2) NOT NULL,
    `IgvAmount` decimal(18,2) NOT NULL,
    `IsBankPayment` smallint NOT NULL,
    `WasBankPaymentUsed` smallint NOT NULL,
    `WasExonerated` smallint NOT NULL,
    `EntityLoadFormatId` char(36) COLLATE ascii_general_ci NULL,
    `IsPartialPayment` smallint NOT NULL,
    `IssueDate` datetime NOT NULL,
    `LateCharge` decimal(18,2) NOT NULL,
    `OperationCodeB` longtext CHARACTER SET utf8mb4 NULL,
    `PaymentDate` datetime NULL,
    `DueDate` datetime NULL,
    `Quantity` decimal(18,2) NOT NULL,
    `Receipt` longtext CHARACTER SET utf8mb4 NULL,
    `Status` tinyint unsigned NOT NULL,
    `SubTotal` decimal(18,2) NOT NULL,
    `Total` decimal(18,2) NOT NULL,
    `Type` tinyint unsigned NOT NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_Payments` PRIMARY KEY (`Id`),
    CONSTRAINT `AK_EconomicManagement_Payments_BankIdentifier` UNIQUE (`BankIdentifier`),
    CONSTRAINT `FK_Payments_AspNetUsers_UserId` FOREIGN KEY (`UserId`) REFERENCES `AspNetUsers` (`Id`),
    CONSTRAINT `FK_Payments_Concepts_ConceptId` FOREIGN KEY (`ConceptId`) REFERENCES `EconomicManagement_Concepts` (`Id`),
    CONSTRAINT `FK_Payments_CurrentAccounts_CurrentAccountId` FOREIGN KEY (`CurrentAccountId`) REFERENCES `EconomicManagement_CurrentAccounts` (`Id`),
    CONSTRAINT `FK_Payments_EntityLoadFormats_EntityLoadFormatId` FOREIGN KEY (`EntityLoadFormatId`) REFERENCES `Admission_EntityLoadFormats` (`Id`),
    CONSTRAINT `FK_Payments_ExternalUsers_ExternalUserId` FOREIGN KEY (`ExternalUserId`) REFERENCES `Generals_ExternalUsers` (`Id`),
    CONSTRAINT `FK_Payments_Invoices_InvoiceId` FOREIGN KEY (`InvoiceId`) REFERENCES `Intranet_Invoices` (`Id`),
    CONSTRAINT `FK_Payments_Payments_ParentPaymentId` FOREIGN KEY (`ParentPaymentId`) REFERENCES `EconomicManagement_Payments` (`Id`),
    CONSTRAINT `FK_Payments_Terms_TermId` FOREIGN KEY (`TermId`) REFERENCES `Enrollment_Terms` (`Id`)
) CHARACTER SET=utf8mb4;

