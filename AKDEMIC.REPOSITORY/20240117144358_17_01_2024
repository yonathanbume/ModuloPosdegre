    `IsExonerated` smallint NOT NULL,
    CONSTRAINT `PK_SpecialCases` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_SpecialCases_AdmissionTypes_AdmissionTypeId` FOREIGN KEY (`AdmissionTypeId`) REFERENCES `Admission_AdmissionTypes` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_SpecialCases_Careers_CareerId` FOREIGN KEY (`CareerId`) REFERENCES `Generals_Careers` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_SpecialCases_Concepts_ForeignConceptId` FOREIGN KEY (`ForeignConceptId`) REFERENCES `EconomicManagement_Concepts` (`Id`),
    CONSTRAINT `FK_SpecialCases_Concepts_PrivateConceptId` FOREIGN KEY (`PrivateConceptId`) REFERENCES `EconomicManagement_Concepts` (`Id`),
    CONSTRAINT `FK_SpecialCases_Concepts_PublicConceptId` FOREIGN KEY (`PublicConceptId`) REFERENCES `EconomicManagement_Concepts` (`Id`)
) CHARACTER SET=utf8mb4;

