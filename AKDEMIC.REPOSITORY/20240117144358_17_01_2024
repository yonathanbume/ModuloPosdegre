    CONSTRAINT `FK_EvaluationProjectAdvances_EvaluationProjects_ProjectId` FOREIGN KEY (`ProjectId`) REFERENCES `Evaluation_Projects` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

