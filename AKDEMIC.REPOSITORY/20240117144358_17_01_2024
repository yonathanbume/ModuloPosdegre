CREATE TABLE `Enrollment_Courses` (
    `Id` char(36) COLLATE ascii_general_ci NOT NULL,
    `AcademicProgramId` char(36) COLLATE ascii_general_ci NULL,
    `AreaId` char(36) COLLATE ascii_general_ci NULL,
    `CareerId` char(36) COLLATE ascii_general_ci NULL,
    `SpecialtyId` char(36) COLLATE ascii_general_ci NULL,
    `CourseComponentId` char(36) COLLATE ascii_general_ci NOT NULL,
    `CourseTypeId` char(36) COLLATE ascii_general_ci NOT NULL,
    `Code` varchar(50) CHARACTER SET utf8mb4 NOT NULL,
    `Credits` decimal(18,2) NOT NULL,
    `Name` varchar(200) CHARACTER SET utf8mb4 NOT NULL,
    `BannerImage` longtext CHARACTER SET utf8mb4 NULL,
    `PracticalHours` tinyint unsigned NOT NULL,
    `SeminarHours` tinyint unsigned NOT NULL,
    `ShortCode` varchar(50) CHARACTER SET utf8mb4 NULL,
    `ShortName` longtext CHARACTER SET utf8mb4 NULL,
    `TheoreticalHours` tinyint unsigned NOT NULL,
    `VirtualHours` tinyint unsigned NOT NULL,
    `RelationId` varchar(50) CHARACTER SET utf8mb4 NULL,
    `DeletedAt` datetime NULL,
    `DeletedBy` longtext CHARACTER SET utf8mb4 NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_Courses` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_Courses_AcademicPrograms_AcademicProgramId` FOREIGN KEY (`AcademicProgramId`) REFERENCES `Generals_AcademicPrograms` (`Id`),
    CONSTRAINT `FK_Courses_Areas_AreaId` FOREIGN KEY (`AreaId`) REFERENCES `Enrollment_Areas` (`Id`),
    CONSTRAINT `FK_Courses_Careers_CareerId` FOREIGN KEY (`CareerId`) REFERENCES `Generals_Careers` (`Id`),
    CONSTRAINT `FK_Courses_CourseComponents_CourseComponentId` FOREIGN KEY (`CourseComponentId`) REFERENCES `TeachingManagement_CourseComponents` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Courses_CourseTypes_CourseTypeId` FOREIGN KEY (`CourseTypeId`) REFERENCES `Enrollment_CourseTypes` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Courses_Specialties_SpecialtyId` FOREIGN KEY (`SpecialtyId`) REFERENCES `Generals_Specialties` (`Id`)
) CHARACTER SET=utf8mb4;

