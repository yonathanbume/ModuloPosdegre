    `WorkerId` char(36) COLLATE ascii_general_ci NOT NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_WorkerTermPayrollDetails` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_WorkerTermPayrollDetails_WageItems_WageItemId` FOREIGN KEY (`WageItemId`) REFERENCES `Payroll_WageItems` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_WorkerTermPayrollDetails_Workers_WorkerId` FOREIGN KEY (`WorkerId`) REFERENCES `Payroll_Workers` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_WorkerTermPayrollDetails_WorkingTerms_WorkingTermId` FOREIGN KEY (`WorkingTermId`) REFERENCES `Payroll_WorkingTerms` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

