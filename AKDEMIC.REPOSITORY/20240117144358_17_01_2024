CREATE TABLE `Intranet_Classes` (
    `Id` char(36) COLLATE ascii_general_ci NOT NULL,
    `ClassroomId` char(36) COLLATE ascii_general_ci NOT NULL,
    `ClassScheduleId` char(36) COLLATE ascii_general_ci NOT NULL,
    `SectionId` char(36) COLLATE ascii_general_ci NOT NULL,
    `UnitActivityId` char(36) COLLATE ascii_general_ci NULL,
    `DictatedDate` datetime NULL,
    `VirtualClassId` char(36) COLLATE ascii_general_ci NULL,
    `ClassNumber` int NOT NULL,
    `EndTime` datetime NOT NULL,
    `IsDictated` smallint NOT NULL,
    `IsRescheduled` smallint NOT NULL,
    `NeedReschedule` smallint NOT NULL,
    `StartTime` datetime NOT NULL,
    `WeekNumber` int NOT NULL,
    `Commentary` longtext CHARACTER SET utf8mb4 NULL,
    `EvaluationId` char(36) COLLATE ascii_general_ci NULL,
    `TeacherId` varchar(255) CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_Classes` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_Classes_Classrooms_ClassroomId` FOREIGN KEY (`ClassroomId`) REFERENCES `Enrollment_Classrooms` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Classes_ClassSchedules_ClassScheduleId` FOREIGN KEY (`ClassScheduleId`) REFERENCES `Intranet_ClassSchedules` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Classes_Evaluations_EvaluationId` FOREIGN KEY (`EvaluationId`) REFERENCES `Enrollment_Evaluations` (`Id`),
    CONSTRAINT `FK_Classes_Sections_SectionId` FOREIGN KEY (`SectionId`) REFERENCES `Enrollment_Sections` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Classes_Teachers_TeacherId` FOREIGN KEY (`TeacherId`) REFERENCES `Generals_Teachers` (`UserId`),
    CONSTRAINT `FK_Classes_UnitActivities_UnitActivityId` FOREIGN KEY (`UnitActivityId`) REFERENCES `TeachingManagement_UnitActivities` (`Id`),
