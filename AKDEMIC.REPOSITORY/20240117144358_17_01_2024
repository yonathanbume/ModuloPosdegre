    `CivilStatus` tinyint unsigned NOT NULL,
    `FirstTime` smallint NULL,
    `PasswordChangeDate` datetime NULL,
    `FullName` varchar(255) CHARACTER SET utf8mb4 NULL,
    `IsActive` smallint NOT NULL,
    `IsLockedOut` smallint NOT NULL,
    `LockedOutReason` longtext CHARACTER SET utf8mb4 NULL,
    `MysqlPasswordHash` longtext CHARACTER SET utf8mb4 NULL,
    `Name` varchar(250) CHARACTER SET utf8mb4 NOT NULL,
    `MaternalSurname` varchar(250) CHARACTER SET utf8mb4 NULL,
    `PaternalSurname` varchar(250) CHARACTER SET utf8mb4 NULL,
