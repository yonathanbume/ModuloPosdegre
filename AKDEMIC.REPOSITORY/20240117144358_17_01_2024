    `ReceiptCode` longtext CHARACTER SET utf8mb4 NULL,
    `DerivedUserId` varchar(255) CHARACTER SET utf8mb4 NULL,
    `FileURL` longtext CHARACTER SET utf8mb4 NULL,
    `QuantityPrinted` int NOT NULL,
    `JsonAcademicYears` longtext CHARACTER SET utf8mb4 NULL,
    `StartAcademicYear` int NULL,
    `EndAcademicYear` int NULL,
    `StartTermId` char(36) COLLATE ascii_general_ci NULL,
    `EndTermId` char(36) COLLATE ascii_general_ci NULL,
    `HtmlContent` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_RecordHistories` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_RecordHistories_AspNetUsers_DerivedUserId` FOREIGN KEY (`DerivedUserId`) REFERENCES `AspNetUsers` (`Id`),
    CONSTRAINT `FK_RecordHistories_Terms_EndTermId` FOREIGN KEY (`EndTermId`) REFERENCES `Enrollment_Terms` (`Id`),
    CONSTRAINT `FK_RecordHistories_Terms_RecordTermId` FOREIGN KEY (`RecordTermId`) REFERENCES `Enrollment_Terms` (`Id`),
    CONSTRAINT `FK_RecordHistories_Terms_StartTermId` FOREIGN KEY (`StartTermId`) REFERENCES `Enrollment_Terms` (`Id`)
) CHARACTER SET=utf8mb4;

