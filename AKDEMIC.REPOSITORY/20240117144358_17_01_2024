    `TutorId` varchar(255) CHARACTER SET utf8mb4 NULL,
    `TutoringStudentStudentId` char(36) COLLATE ascii_general_ci NOT NULL,
    `TutoringStudentTermId` char(36) COLLATE ascii_general_ci NOT NULL,
    `Observation` longtext CHARACTER SET utf8mb4 NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_TutoringAttendances` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_TutoringAttendances_SupportOffices_SupportOfficeId` FOREIGN KEY (`SupportOfficeId`) REFERENCES `Tutoring_SupportOffice` (`Id`),
    CONSTRAINT `FK_TutoringAttendances_SupportOfficeUsers_SupportOfficeUserId` FOREIGN KEY (`SupportOfficeUserId`) REFERENCES `Tutoring_SupportOfficeUser` (`UserId`),
