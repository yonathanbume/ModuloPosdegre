    `AgreementTypeId` char(36) COLLATE ascii_general_ci NOT NULL,
    CONSTRAINT `PK_AcademicAgreementTypes` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_AcademicAgreementTypes_AcademicAgreements_AcademicAgreementId` FOREIGN KEY (`AcademicAgreementId`) REFERENCES `AcademicExchange_AcademicAgreements` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_AcademicAgreementTypes_AgreementTypes_AgreementTypeId` FOREIGN KEY (`AgreementTypeId`) REFERENCES `AcademicExchange_AgreementTypes` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

