    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_Postulations` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_Postulations_AspNetUsers_UserId` FOREIGN KEY (`UserId`) REFERENCES `AspNetUsers` (`Id`),
    CONSTRAINT `FK_Postulations_PostulationInformations_PostulationInformationId` FOREIGN KEY (`PostulationInformationId`) REFERENCES `AcademicExchange_PostulationInformations` (`Id`),
    CONSTRAINT `FK_Postulations_Questionnaires_QuestionnaireId` FOREIGN KEY (`QuestionnaireId`) REFERENCES `AcademicExchange_Questionnaires` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_Postulations_Scholarships_ScholarshipId` FOREIGN KEY (`ScholarshipId`) REFERENCES `AcademicExchange_Scholarships` (`Id`) ON DELETE RESTRICT
) CHARACTER SET=utf8mb4;

