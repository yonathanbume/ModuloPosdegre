    `Size` bigint NOT NULL,
    `Status` int NOT NULL,
    `Title` longtext CHARACTER SET utf8mb4 NULL,
    `Type` smallint NOT NULL,
    `Observation` longtext CHARACTER SET utf8mb4 NULL,
    `FundingSource` int NOT NULL,
    `IsPaid` smallint NOT NULL,
    `GeneratedId` int NOT NULL AUTO_INCREMENT,
