    `Code` longtext CHARACTER SET utf8mb4 NULL,
    `DeletedAt` datetime NULL,
    `DeletedBy` longtext CHARACTER SET utf8mb4 NULL,
    `CreatedAt` datetime NULL,
    `CreatedBy` longtext CHARACTER SET utf8mb4 NULL,
    `UpdatedAt` datetime NULL,
    `UpdatedBy` longtext CHARACTER SET utf8mb4 NULL,
    CONSTRAINT `PK_InvestigationProjects` PRIMARY KEY (`Id`),
    CONSTRAINT `FK_InvestigationProjects_AcademicPrograms_AcademicProgramId` FOREIGN KEY (`AcademicProgramId`) REFERENCES `Generals_AcademicPrograms` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_InvestigationProjects_AspNetUsers_CoordinatorId` FOREIGN KEY (`CoordinatorId`) REFERENCES `AspNetUsers` (`Id`),
    CONSTRAINT `FK_InvestigationProjects_Careers_CareerId` FOREIGN KEY (`CareerId`) REFERENCES `Generals_Careers` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_InvestigationProjects_InvestigationProjectRubrics_AdvanceRub~` FOREIGN KEY (`AdvanceRubricId`) REFERENCES `Investigation_ProjectRubrics` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_InvestigationProjects_InvestigationProjectRubrics_FinalRubri~` FOREIGN KEY (`FinalRubricId`) REFERENCES `Investigation_ProjectRubrics` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_InvestigationProjects_ResearchLines_ResearchLineId` FOREIGN KEY (`ResearchLineId`) REFERENCES `Investigation_ResearchLines` (`Id`) ON DELETE RESTRICT,
    CONSTRAINT `FK_InvestigationProjects_Sections_SectionId` FOREIGN KEY (`SectionId`) REFERENCES `Enrollment_Sections` (`Id`)
) CHARACTER SET=utf8mb4;

